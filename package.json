{
  "name": "url-to-pdf-api",
  "version": "1.0.0",
  "description": "Web page PDF rendering done right. Packaged to an easy API.",
  "overrides": {
    "diff": "^8.0.3"
  },
  "main": "src/index.js",
  "engines": {
    "node": ">=24.x.x"
  },
  "scripts": {
    "start": "env-cmd -- nodemon --watch ./src -e js src/index.js",
