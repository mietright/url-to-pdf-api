    "test": "mocha --timeout 10000",
    "fix-lint": "eslint ./src ./test --fix",
    "fix-fmt": "prettier -c ./src --config ./.prettierrc ./test --write",
    "lint": "eslint ./src ./test",
    "fmt": "prettier -c ./src ./test --config ./.prettierrc",
    "prod:start": "node dist/index.js"
  },
  "repository": {
