    "@babel/register": "^7.28.6",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^10.0.1",
    "@isaacs/brace-expansion": ">=5.0.1",
    "chai": "^6.2.2",
    "env-cmd": "^11.0.0",
    "eslint": "^10.0.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
