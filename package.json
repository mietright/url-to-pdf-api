  },
  "homepage": "https://github.com/alvarcarto/url-to-pdf-api#readme",
  "dependencies": {
    "body-parser": "^2.2.2",
    "compression": "^1.8.1",
    "core-js": "^3.48.0",
    "cors": "^2.8.6",
    "express": "^5.2.1",
    "express-prom-bundle": "^8.0.0",
    "express-validation": "^4.1.1",
    "lodash": "^4.17.23",
    "morgan": "^1.10.1",
    "normalize-url": "^9.0.0",
    "pdf-parse": "^2.4.5",
